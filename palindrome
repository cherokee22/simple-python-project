str1 = str(input(" enter the string"))
c = str1[::-1]
print(c)
if(str1==c):
    print("is palendrome ")
else :
    print("not palendrome")




    num = int(input("Enter a number: "))
# Input: 12321
temp = num
reverse = 0
while temp > 0:
    remainder = temp % 10
    reverse = (reverse * 10) + remainder
    temp = temp // 10
if num == reverse:
  print('Palindrome')
else:
  print("Not Palindrome")
# Output: Palindrome
